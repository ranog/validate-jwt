{
  "family": "validate-jwt",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::231711413035:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "validate-jwt",
      "image": "<IMAGE>",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
